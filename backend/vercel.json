{
    "version": 2,
    "builds": [
      {
        "src": "backend/api/*.js",  
        "use": "@vercel/node"      
      }
    ],
    "routes": [
      {
        "src": "/api/user",
        "dest": "/backend/api/user.js"  
      },
      {
        "src": "/api/admin",
        "dest": "/backend/api/admin.js"  
      },
      {
        "src": "/api/company",
        "dest": "/backend/api/company.js" 
      }
    ]
  }
  